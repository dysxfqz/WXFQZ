<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.suteam.html.permission.model.UserPermission" table="user_permission">
		<id name="id" column="id" type="string">
			<generator class="uuid" />
		</id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="url" column="url" type="java.lang.String" />
		<property name="type" column="type" type="java.lang.String" />
		<property name="description" column="description" type="java.lang.String" />
		<property name="moduleId" column="module_id" type="java.lang.String" />
		<property name="moduleType" column="module_type" type="java.lang.Integer" />
	</class>
</hibernate-mapping>
